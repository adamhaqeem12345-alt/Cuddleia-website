# Database Configuration
# PostgreSQL connection string for storing products, orders, and user data
DATABASE_URL=postgresql://user:password@localhost:5432/pinkstore

# PayPal Configuration
# Get these from https://developer.paypal.com/
PAYPAL_CLIENT_ID=your_paypal_client_id_here
PAYPAL_CLIENT_SECRET=your_paypal_client_secret_here
# Use 'sandbox' for testing, 'live' for production
PAYPAL_MODE=sandbox

# AliExpress Configuration
# Get these from https://portals.aliexpress.com/
ALIEXPRESS_API_KEY=your_aliexpress_api_key_here
ALIEXPRESS_API_SECRET=your_aliexpress_api_secret_here

# Next.js Configuration
# URL of your application
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Optional: Webhook secrets for payment notifications
PAYPAL_WEBHOOK_ID=your_webhook_id_here
